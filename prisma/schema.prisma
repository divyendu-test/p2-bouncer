generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  data  String   @default("")
  date  DateTime
  id    String   @id
  slug  String   @default("") @unique
  title String   @default("")
  user  User
}

model User {
  email String @default("") @unique
  id    String @id
  name  String @default("")
  post  Post[]
}